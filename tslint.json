{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-plugin-prettier",
    "tslint-config-prettier"
  ],
  "rules": {
    "prettier": [
      true,
      {
        "printWidth": 120,
        "singleQuote": true,
        "trailingComma": "all"
      }
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "no-console": [false, "log"],
    "object-literal-sort-keys": false,
    "member-access": [
      true,
      "no-public"
    ],
    "no-namespace": [
      true,
      "allow-declarations"
    ],
    "jsx-no-lambda": false
  }
}
